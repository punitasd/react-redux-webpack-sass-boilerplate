{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "start": "webpack --config webpack.prod.js && node ./dist/server.js",
    "start-prod": "node ./server.js",
    "build": "rm -rf dist &&webpack --mode production --config webpack.prod.js&& webpack --mode production --config webpack.server.js",
    "start-dev": "webpack-dev-server --open --hot --inline --config webpack.dev.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg|png)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./client/setupTests.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.12.16",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "express": "^4.17.1",
    "file-loader": "^4.1.0",
    "fs": "0.0.1-security",
    "history": "^4.10.1",
    "https": "^1.0.0",
    "i": "^0.3.6",
    "identity-obj-proxy": "^3.0.0",
    "install": "^0.13.0",
    "ismobilejs": "^0.5.1",
    "minimist": "^1.2.0",
    "npm": "^6.13.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-split-pane": "^2.0.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  }
